<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - OrganizadorPessoal - Doodor</title>

    <link rel="shortcut icon" href="favicon.png">

    <environment include="Development">
        <link href="~/css/bootstrap.min.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/font-awesome/css/font-awesome.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/css/plugins/toastr/toastr.min.css" rel="stylesheet" asp-append-version="true" />       
        <link href="~/js/plugins/gritter/jquery.gritter.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/css/animate.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/css/style.css" rel="stylesheet" asp-append-version="true" />
    </environment>
    <environment exclude="Development">
        <link href="~/css/bootstrap.min.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/font-awesome/css/font-awesome.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/css/plugins/toastr/toastr.min.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/js/plugins/gritter/jquery.gritter.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/css/animate.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/css/style.css" rel="stylesheet" asp-append-version="true" />
    </environment>
</head>
<body>
    <div id="wrapper">
        <nav class="navbar-default navbar-static-side" role="navigation">
            <div class="sidebar-collapse">
                <ul class="nav metismenu" id="side-menu">
                    <li class="nav-header">
                        <div class="dropdown profile-element">
                            <span>
                                <img alt="image" class="img-circle" src="~/images/profile_small.jpg" />
                            </span>
                            <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                                <span class="clear">
                                    <span class="block m-t-xs">
                                        <strong class="font-bold">Pedro Vinicius</strong>
                                    </span> <span class="text-muted text-xs block">Developer <b class="caret"></b></span>
                                </span>
                            </a>
                            <ul class="dropdown-menu animated fadeInRight m-t-xs">
                                <li><a href="#">Profile</a></li>
                                <li class="divider"></li>
                                <li>
                                    <a asp-area="" asp-controller="Account" asp-action="Logout">Log out</a>                                    
                                </li>
                            </ul>
                        </div>
                        <div class="logo-element">
                            CP
                        </div>
                    </li>
                    <li class="active">
                        <a href="#">
                            <i class="fa fa-usd"></i>
                            <span class="nav-label">Financeiro</span>
                        </a>
                        <ul class="nav nav-second-level">
                            <li class="active">
                                <a asp-controller="Contas" asp-action="Index">
                                    <i class="icon fa fa-circle-o"></i>
                                    <span>Contas</span>
                                </a>
                            </li>
                        </ul>
                    </li>
                </ul>

            </div>
        </nav>
    </div>        
        <!-- /Header -->
        <!-- Main container -->
    <div id="page-wrapper" class="gray-bg dashbard-1">
        <div class="row border-bottom">
            <nav class="navbar navbar-static-top" role="navigation" style="margin-bottom: 0">
                <div class="navbar-header">
                    <a class="navbar-minimalize minimalize-styl-2 btn btn-primary " href="#"><i class="fa fa-bars"></i> </a>  
                </div>                
            </nav>
        </div>
        @RenderBody()
    </div>
            
        
        <!-- /Main container -->

        <environment include="Development">
            <script src="~/js/jquery-3.1.1.min.js" asp-append-version="true"></script>
            <script src="~/js/bootstrap.min.js" asp-append-version="true"></script>
            <script src="~/js/plugins/metisMenu/jquery.metisMenu.js" asp-append-version="true"></script>
            <script src="~/js/plugins/slimscroll/jquery.slimscroll.min.js" asp-append-version="true"></script>
            <script src="~/js/plugins/flot/jquery.flot.js" asp-append-version="true"></script>
            <script src="~/js/plugins/flot/jquery.flot.tooltip.min.js" asp-append-version="true"></script>
            <script src="~/js/plugins/flot/jquery.flot.spline.js" asp-append-version="true"></script>
            <script src="~/js/plugins/flot/jquery.flot.resize.js" asp-append-version="true"></script>
            <script src="~/js/plugins/flot/jquery.flot.pie.js" asp-append-version="true"></script>
            <script src="~/js/plugins/peity/jquery.peity.min.js" asp-append-version="true"></script>
            <script src="~/js/demo/peity-demo.js" asp-append-version="true"></script>
            <script src="~/js/inspinia.js" asp-append-version="true"></script>
            <script src="~/js/plugins/pace/pace.min.js" asp-append-version="true"></script>
            <script src="~/js/plugins/jquery-ui/jquery-ui.min.js" asp-append-version="true"></script>
            <script src="~/js/plugins/gritter/jquery.gritter.min.js" asp-append-version="true"></script>
            <script src="~/js/plugins/sparkline/jquery.sparkline.min.js" asp-append-version="true"></script>
            <script src="~/js/demo/sparkline-demo.js" asp-append-version="true"></script>
            <script src="~/js/plugins/chartJs/Chart.min.js" asp-append-version="true"></script>
            <script src="~/js/plugins/toastr/toastr.min.js" asp-append-version="true"></script>            
        </environment>
        <environment exclude="Development">
            <script src="~/js/jquery-3.1.1.min.js" asp-append-version="true"></script>
            <script src="~/js/bootstrap.min.js" asp-append-version="true"></script>
            <script src="~/js/plugins/metisMenu/jquery.metisMenu.js" asp-append-version="true"></script>
            <script src="~/js/plugins/slimscroll/jquery.slimscroll.min.js" asp-append-version="true"></script>
            <script src="~/js/plugins/flot/jquery.flot.js" asp-append-version="true"></script>
            <script src="~/js/plugins/flot/jquery.flot.tooltip.min.js" asp-append-version="true"></script>
            <script src="~/js/plugins/flot/jquery.flot.spline.js" asp-append-version="true"></script>
            <script src="~/js/plugins/flot/jquery.flot.resize.js" asp-append-version="true"></script>
            <script src="~/js/plugins/flot/jquery.flot.pie.js" asp-append-version="true"></script>
            <script src="~/js/plugins/peity/jquery.peity.min.js" asp-append-version="true"></script>
            <script src="~/js/demo/peity-demo.js" asp-append-version="true"></script>
            <script src="~/js/inspinia.js" asp-append-version="true"></script>
            <script src="~/js/plugins/pace/pace.min.js" asp-append-version="true"></script>
            <script src="~/js/plugins/jquery-ui/jquery-ui.min.js" asp-append-version="true"></script>
            <script src="~/js/plugins/gritter/jquery.gritter.min.js" asp-append-version="true"></script>
            <script src="~/js/plugins/sparkline/jquery.sparkline.min.js" asp-append-version="true"></script>
            <script src="~/js/demo/sparkline-demo.js" asp-append-version="true"></script>
            <script src="~/js/plugins/chartJs/Chart.min.js" asp-append-version="true"></script>
            <script src="~/js/plugins/toastr/toastr.min.js" asp-append-version="true"></script>
        </environment>

        @RenderSection("Scripts", required: false)
</body>
</html>
