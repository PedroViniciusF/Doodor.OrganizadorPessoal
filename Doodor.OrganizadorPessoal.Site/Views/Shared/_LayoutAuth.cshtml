@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - OrganizadorPessoal - Doodor</title>

    <link rel="shortcut icon" href="favicon.png">

    <environment include="Development">
        <link href="~/css/bootstrap.min.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/font-awesome/css/font-awesome.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/css/animate.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/css/style.css" rel="stylesheet" asp-append-version="true" />
    </environment>
    <environment exclude="Development">
        <link href="~/css/bootstrap.min.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/font-awesome/css/font-awesome.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/css/animate.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/css/style.css" rel="stylesheet" asp-append-version="true" />
    </environment>
</head>
<body class="gray-bg">
    <!-- Main container -->
    
    @RenderBody()
    <!-- /Main container -->

    @RenderSection("Scripts", required: false)
</body>
</html>

